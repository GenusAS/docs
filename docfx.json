{
  "build": {
    "content": [
      {
        "files": [
          "users/**.md",
          "users/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "developers/**.md",
          "developers/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/.jpg",
          "**/.gif",
          "**/.png",
          "developers/**.jpg",
          "developers/**.gif",
          "developers/**.png",
          "users/**.jpg",
          "users/**.gif",
          "users/**.png"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "noLangKeyword": false
  }
}
{
  "build": {
    "content": [
      {
        "files": [
          "**.*md",
          "**.*yml"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.jpg",
          "**/*.gif",
          "**/*.png"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "globalMetadata": {
      "_enableSearch": true
    },
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "noLangKeyword": false
  }
}